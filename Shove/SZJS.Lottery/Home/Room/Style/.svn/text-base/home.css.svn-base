#tags
{
	padding-right: 0px;
	padding-left: 0px;
	padding-bottom: 0px;
	margin: 0px 0px 0px 0px;
	width: 700px;
	padding-top: 0px;
	height: 28px;
}
#tags LI
{
	background: url(../Images/con_a_bg2.jpg) no-repeat;
	float: left;
	margin-right: 10px;
	list-style-type: none;
	height: 28px;
	width: 101px;
}
#tags LI a
{
	text-align: center;
	float: left;
	padding-bottom: 0px;
	color: #333333;
	line-height: 28px;
	padding-top: 0px;
	height: 28px;
	text-decoration: none;
	font-size: 12px;
	font-weight: bold;
	display: block;
	width: 101px;
}
#tags LI.emptyTag
{
	background: none transparent scroll repeat 0% 0%;
	width: 4px;
}
#tags LI.selectTag
{
	margin-bottom: -2px;
	position: relative;
	height: 30px;
}
#tags LI.selectTag A
{
	background: url(../Images/con_a_bg.jpg) no-repeat;
	color: #000;
	line-height: 30px;
	height: 30px;
}
#tagContent
{
	border-right: #aecbd4 1px solid;
	padding-right: 1px;
	border-top: #aecbd4 1px solid;
	padding-left: 1px;
	padding-bottom: 1px;
	border-left: #aecbd4 1px solid;
	border-bottom: #aecbd4 1px solid;
	background-color: #fff;
	border-color: #dfdfdf;
}
.tagContent
{
	padding-right: 10px;
	display: none;
	padding-left: 10px;
	background: url(../Images/con_a_bg3.jpg) repeat-x;
	padding-bottom: 10px;
	width: 754px;
	color: #474747;
	padding-top: 10px;
	overflow: hidden;
	padding-right: 10px;
}
#tagContent DIV.selectTag
{
	display: block;
}

