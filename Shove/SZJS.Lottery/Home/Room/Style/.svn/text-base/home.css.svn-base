.wrap
{
	width: 986px;
	margin: 7px auto 0px auto;
	overflow: hidden;
}
.wrap .lefts
{
	background-position: left -10px;
	float: left;
	width: 196px;
	overflow: hidden;
	background-image: url( '../Images/home_left_bg.jpg' );
	background-repeat: no-repeat;
	padding-top: 10px;
}
.wrap #lefts
{
	background-image: url( '../Images/home_left_bg.jpg' );
	background-repeat: no-repeat;
	background-position: left top;
	height: 10px;
	font-size: 0px;
	padding: 0px;
	overflow: hidden;
}
.wrap .lefts ul
{
	border-style: none solid none solid;
	border-width: 1px;
	border-color: #f4b991;
	background-color: #ffede3;
	padding-left: 30px;
	padding-right: 10px;
}
.wrap .lefts ul li h1
{
	background-position: left bottom;
	font-size: 14px;
	font-weight: bold;
	color: #9a3903;
	background-image: url( '../Images/home_left_bg2.jpg' );
	background-repeat: no-repeat;
	padding-bottom: 7px;
	padding-left: 20px;
}
.wrap .lefts ul li
{
	padding-top: 7px;
}
.wrap .lefts ul li a
{
	padding-left: 20px;
}
.wrap .rights
{
	width: 780px;
	overflow: hidden;
	float: right;
}
.wrap .rights .myself
{
	border: 1px dashed #ff6fa0;
	width: 778px;
	background-color: #fbeae8;
	overflow: hidden;
	padding-bottom: 15px;
	margin-bottom: 10px;
}
.wrap .rights .myself dl
{
	float: left;
}
.wrap .rights .myself dl img
{
	border: 1px solid #999999;
	margin-left: 30px;
	margin-top: 10px;
	padding: 5px;
	background-color: #FFFFFF;
}
.wrap .rights .myself ul
{
	float: left;
	margin-left: 20px;
	margin-top: 10px;
	width: 400px;
}
.wrap .rights .myself ul li
{
	display: block;
	height: 22px;
	font-family: 宋体, Arial, Helvetica, sans-serif;
	margin-top: 5px;
}
.wrap .rights .myself ul li span
{
	font-weight: bold;
	color: Red;
	margin-left: 5px;
	margin-right: 5px;
}
.wrap .rights .myself ul li em
{
	color: #000099;
}
.wrap .rights .myself ul li img
{
	padding-bottom: 4px;
}
.wrap .rights .myself p
{
	margin-top: 15px;
	float: left;
}
.wrap .rights .myself p .home_cz
{
	background-image: url( '../Images/home_tijiao.jpg' );
	background-repeat: no-repeat;
	background-position: left center;
	width: 45px;
	height: 22px;
	display: inline-block;
	margin-left: 50px;
}
.wrap .rights .myself p .home_tk
{
	background-image: url( '../Images/home_tijiao.jpg' );
	background-repeat: no-repeat;
	background-position: right center;
	width: 45px;
	height: 22px;
	display: inline-block;
	margin-left: 20px;
}
.wrap .rights .jilu
{
	border: 1px solid #dbdbdb;
	margin-top: 10px;
}


.wrap .rights .jilu .title
{
	background-image: url( '../Images/home_my_bg.jpg' );
	background-repeat: repeat-x;
	height: 28px;
	overflow: hidden;
	line-height: 28px;
}
.wrap .rights .jilu .title h1
{
	color: #666666;
	padding-left: 10px;
	font-size: 14px;
	font-weight: bold;
	float: left;
}
.wrap .rights .jilu .title a
{
	float: right;
	margin-right: 10px;
}
.wrap .rights .jilu table .col td
{
	background-color: #f8f1f4;
}

